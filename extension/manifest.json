{
  "manifest_version": 3,
  "name": "Web Env",
  "version": "1.0.0",
  "description": "A tool to easily define browser environment variables",
  "author": "Gj",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Web Env",
    "default_popup": "page/popups.html",
    "default_icon": {
      "32": "icons/32.png",
      "48": "icons/48.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
  },
  "devtools_page": "devtools/devtools.html",
  "web_accessible_resources": [
    {
      "resources": [
        "document.js",
        "dashboard/dashboard.umd.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}